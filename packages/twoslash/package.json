{
  "name": "nopends-twoslash",
  "version": "3.1.3",
  "description": "Typescript Twoslash Integration for Fumadocs",
  "keywords": [
    "NextJs",
    "nopends",
    "Docs"
  ],
  "homepage": "https://docs.nopends.com",
  "repository": "github:nopends/nopends",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "exports": {
    "./twoslash.css": "./styles/twoslash.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./cache-fs": {
      "import": "./dist/cache-fs.js",
      "types": "./dist/cache-fs.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "styles/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.13",
    "@shikijs/twoslash": "^3.4.2",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.1.0",
    "mdast-util-to-hast": "^13.2.0",
    "tailwind-merge": "^3.3.0",
    "twoslash": "^0.3.1"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.3",
    "@types/node": "22.15.19",
    "@types/react": "^19.1.4",
    "eslint-config-custom": "workspace:*",
    "nopends-ui": "workspace:*",
    "shiki": "^3.4.2",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "nopends-ui": "^15.0.0",
    "react": "18.x.x || 19.x.x",
    "shiki": "1.x.x || 2.x.x || 3.x.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
